@model IEnumerable<KickballManager.Models.Player>
@using KickballManager.Models

@if (Model != null)
{
    <div class="grid">
        <table cellspacing="0" width="80">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Gender
                    </th>
                   

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td align="left">
                            @item.Name
                        </td>
                        <td align="left">
                            @item.Gender
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>

} 